    "start": "micro",
    "test": "ava",
    "watch": "nodemon --watch . --exec \"npm run start\""
  },
  "keywords": [],
  "author": "Joseph Fraley",
  "license": "MIT",
  "dependencies": {
    "axios": "0.16.2",
    "cheerio": "1.0.0-rc.2",
    "date-fns": "1.28.5",
    "micro": "8.0.4",
    "ramda": "0.24.1"
  },
  "devDependencies": {
    "@joefraley/eslint-config": "0.0.2",
    "ava": "0.22.0",
    "babel-eslint": "7.2.3",
    "codecov": "2.3.0",
    "concurrently": "3.5.0",
    "eslint": "4.6.1",
    "husky": "0.14.3",
    "nodemon": "1.11.0",
    "nyc": "11.1.0",
    "prettier": "1.6.1",
    "request-promise": "4.2.1",
    "test-listen": "1.0.2"
  }
}
